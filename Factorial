#include  <stdio.h>
#include <stdlib.h>
#include <string.h>

int fact(int n) {
    int temp = 1;
    if (n == 0 || n == 1) {
        return 1;

    } else {
        return n * fact(n - 1);

    }
}

int main(){
    int sayi;
    printf("Kac Faktoriyel:");
    scanf("%d",&sayi);
    printf("sonuc :%d",fact(sayi));

}
